{
  "manifest_version": 3,
  "name": "Google Meet Tracker",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "48": "images/favicon-32x32.png",
      "128": "images/android-chrome-192x192.png"
    }
  }
}
