{
    "manifest_version": 3,
    "name": "Google Meet Tracker",
    "version": "1.0",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "dist/background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  